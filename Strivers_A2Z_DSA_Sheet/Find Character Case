import java.util.*;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
        String s = sc.next();
        char ch = s.charAt(0);
        if(Character.isUpperCase(ch))
        {
            System.out.print("1");
        }
        else if(Character.isLowerCase(ch))
        {
            System.out.print("0");
        }
        else
        {
            System.out.print("-1");
        }

    }
}
